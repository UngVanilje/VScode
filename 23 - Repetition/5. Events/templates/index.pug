html
    head
        title Express-app 3
        script(src='index.js')
    body
        h1 Express-app
        input(type='text', id='tlf', placeholder='Telefonnummer')
        table
            tr
                th Navn
                th Telefonnummer
            tbody(id='tbody')
                each person in people
                    tr
                        td #{person.Navn}
                        td
                            a(id="link" href='http://localhost:8080' onclick=`changeNumber(${person.Telefon})`) #{person.Telefon}
